            <!-- New Candidate Modal -->
            <div class="modal fade" id="newCandidateModal" tabindex="-1" role="dialog" aria-labelledby="newCandidateModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg" style="position:unset">
                    <div class="modal-content" style="position:unset">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                            <h4 class="modal-title" id="newCandidateModalLabel">Create New Candidate</h4>
                        </div>
                        <div class="modal-body">
                            <div id="candidateId" hidden></div>
                            @Html.Label("Candidate's Name")
                            <br />
                            <div class="row">
                                <div class="col-md-4">
                                    <input type="text" id="firstName" placeholder="First Name">
                                </div>
                                <div class="col-md-4">
                                    <input type="text" id="middleName" placeholder="Middle Name">
                                </div>
                                <div class="col-md-4">
                                    <input type="text" id="lastName" placeholder="Last Name">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <label for="Name">Email</label>
                                    <input type="text" class="form-control" id="email" placeholder="Email">
                                </div>

                                <div class="col-md-6">
                                    <label for="Name">Phone number</label>
                                    <input type="text" class="form-control" id="phoneNumber" placeholder="Phone Number">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <label for="Name">Job Title</label>
                                    <input type="text" class="form-control" id="jobTitle" placeholder="Job Title">
                                </div>

                                <div class="col-md-6">
                                    <label>Job Level</label>
                                    <div class="form-group">
                                        <select class="form-control select2" id="jobLevel">
                                            <option value="Fresher">Fresher</option>
                                            <option value="Junior">Junior</option>
                                            <option value="Senior">Senior</option>
                                            <option value="Manager">Manager</option>
                                            <option value="Higher">Higher</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-primary" onclick="return _add();" id="btnAdd">Add</button>
                                <button type="button" class="btn btn-primary" onclick="return _edit();" id="btnUpdate" style="display:none">Update</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Candidate's information Modal -->
            <div class="modal fade" id="candiateInfoModal" tabindex="-1" role="dialog" aria-hidden="true">
                <div class="modal-dialog modal-lg" style="position:unset">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                            <h4 class="modal-title" id="infoCandidateModalLabel">Candidate information</h4>
                        </div>
                        <div class="modal-body">
                            <div> This is login information you can send to candidate via email to require to complete forms</div>
                            <div>User Name:&nbsp;<span id="userNameInfo"></span></div>
                            <div>Password:&nbsp;<span id="passwordRaw"></span></div>
                            <div>Please update information before:&nbsp;<span id="lockoutDateUtc"></span></div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-primary" data-dismiss="modal">OK</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

