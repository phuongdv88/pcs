@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title id="title">@ViewBag.SpecialistName</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/Content/Css/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>

    <!-- Bootstrap 3.3.7 -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link href="~/Content/Ionicons/css/ionicons.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/Content/Css/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/Css/skins/_all-skins.min.css" rel="stylesheet" />
    <link href="~/Content/iCheck/all.css" rel="stylesheet" />

    <link href="~/Content/Css/logo.css" rel="stylesheet" />
    <link href="~/Content/Css/processBackgroundChecksCandidate.css" rel="stylesheet" />
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>

<body class="hold-transition skin-blue sidebar-mini" style="padding-top:0px">
    <!--Header menu-->
    <div class="row col-md-12 col-sm-12 col-xs-12  header" style="margin:0;padding:0">
        <div class="col-md-3 col-sm-3 col-xs-3 " style=" background-color: #464646;padding: 0px;height:100%;">
            <img src="~/Content/Image/logo PCS trang.png" style="height:100%" />
        </div>
        <div class="col-md-9 col-sm-9 col-xs-9 " style=" background-color: #464646;padding: 0px;height:100%">

            <!-- Control Sidebar Toggle Button -->
            <div style="float: right;padding-top: 15px;padding-left: 15px;padding-right:1em" class="dropdown">
                <a href="#" data-toggle="control-sidebar">
                    <i class="fa fa-gears"></i>
                </a>
                <div class="dropdown-content sidenav">
                    <a href="#" data-toggle="modal" onclick="return getProfile();">
                        <b>
                            <i class="fa fa-user"></i> Change profile
                        </b>
                    </a>

                    <a href="#" data-toggle="modal" data-target="#changePassModal">

                        <b>
                            <i class="fa fa-edit"></i>Change Password
                        </b>
                    </a>

                    <a href=@Url.Action("Login", "Home")>
                        <b>
                            <i class="glyphicon glyphicon-log-out"></i>Log out
                        </b>
                    </a>
                </div>
            </div>
            <!-- form edit pass -->
            <div class="modal fade" id="changePassModal" role="dialog" tabindex="-1">
                <div class="modal-dialog" style="position:unset">
                    <!-- Modal content-->
                    <div class="modal-content" style="width: 100%;padding: 2em 2em 0 2em">
                        <div class="modal-header" style="padding-left: 0px;">
                            <h4>Change PassWord</h4>
                        </div>
                        <form onsubmit="updatePassword(); return false;">
                            <div class="modal-body form-edit">
                                <input id="UserName" name="UserName" hidden>
                                <label>Old Password</label><br />
                                <input type="password" id="oldPassword" placeholder="Password old.." required>
                                <label>New Password</label><br />
                                <input type="password" id="newPassword" placeholder="Password.." required>
                                <label>Confirm Password</label><br />
                                <input type="password" id="confirmPassword" placeholder="Confirm password.." required>
                                <div id="changePasswordMessage"></div>
                                <div class="modal-footer">
                                    <button type="submit" class="btn btn-primary" id="btnEditPass">Save</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- end form edit pass -->
            <!-- form uploadFile -->
            <div class="modal fade" id="uploadFileModal" role="dialog" tabindex="-1">
                <div class="modal-dialog" style="position:unset">
                    <!-- Modal content-->
                    <div class="modal-content" style="width: 100%;padding: 2em 2em 0 2em">
                        <div class="modal-header" style="padding-left: 0px;">
                            <h4>Upload Report</h4>
                        </div>
                        <div class="modal-body form-edit">
                            <input id="candidateId" name="candidateId" hidden>
                            <!--Form upload File-->
                            <div class="row">
                                <input class="col-md-12" id="fileInput" type="file" accept="application/pdf">
                            </div>
                            <div class="row">
                                <label>Change Status</label>
                                <div class="form-group col-md-12">
                                    <select class="form-control select2" id="candidateStatus" required>
                                        <option value="Completed">Completed</option>
                                        <option value="Closed">Closed</option>
                                    </select>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-primary" id="btnUpload" onclick="uploadFile(); return false;">Upload file</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- end form uploadFile -->
            <!-- form edit information -->
            <div class="modal fade" id="changeProfileModal" role="dialog" tabindex="-1">
                <div class="modal-dialog modal-lg" style="position:unset">
                    <!-- Modal content-->
                    <div class="modal-content" style="width: 100%;padding:  2em 2em 0 2em">
                        <div class="modal-header" style="padding-left: 0px;">
                            <h4 class="modal-title">Change Profile</h4>
                        </div>
                        <form onsubmit="editProfile(); return false;">

                            <div class="modal-body form-edit">
                                <label>First name</label><br />
                                <input type="text" id="specialistFirstName" placeholder="Firstname.." required>
                                <label>Middle name</label><br />
                                <input type="text" id="specialistMiddleName" placeholder="Firstname..">
                                <label>Last name</label><br />
                                <input type="text" id="specialistLastName" placeholder="Lastname.." required>
                                <label> Email</label><br />
                                <input type="email" id="specialistEmail" placeholder="Email.." required>
                                <label>Phone number</label><br />
                                <input type="text" id="specialistPhoneNumber" pattern="^[0-9\-\+]{9,15}$" placeholder="Phone number.." required>
                                <div class="modal-footer">
                                    <button type="submit" class="btn btn-primary" id="btnEditProfile">Save</button>
                                    @*<input />*@
                                </div>
                            </div>

                        </form>
                    </div>
                </div>
            </div>

            <!-- end form edit information -->

            <div style="float: right;padding-top: 13px;height: 100%;width:auto">
                <!-- The user image in the navbar-->
                <i class="fa fa-user" style="color:silver" id="welcomeUser"> Welcome @ViewBag.SpecialistName</i>
                <span></span>
            </div>
        </div>
    </div>
    <!-- body-->
    <div class="col-md-12 " style="min-height: 90%;width: 100%;background: #ecf0f5;padding-top: 1em;padding-bottom: 2em;">
        <!-- general information -->
        <div class=" col-md-12">
            <h3>General Information</h3>
        </div>
        <div class=" col-md-12 clearMaginPadding box" style="padding-top: 1em">

            <div class="row col-md-12 ">

                <div class="col-md-8 ">
                    <div class="row">
                        <div class="col-lg-6">
                            <label>Name:</label>
                            <label style="font-weight:bold;" id="candidateFullName"></label>
                        </div>
                        <div class="col-lg-6">
                            <label>Gender:</label>
                            <label style="font-weight:bold;" id="candidateGender"></label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <label>Phone:</label>
                            <label style="font-weight:bold;" id="candidatePhone"></label>
                        </div>
                        <div class="col-lg-6">
                            <label>Email:</label>
                            <label style="font-weight:bold;" id="candidateEmail"></label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <label> DOB:</label>
                            <label style="font-weight:bold;" id="candidateDOB"></label>
                        </div>
                        <div class="col-lg-6">
                            <label>ID Number:</label>
                            <label style="font-weight:bold;" id="candidateIDNumber"></label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <label>Job title:</label>
                            <label style="font-weight:bold;" id="candidateJobTitle"></label>
                        </div>
                        <div class="col-lg-6">
                            <label>Job level:</label>
                            <label style="font-weight:bold;" id="candidateJobLevel"></label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <label>Address:</label>
                            <label style="font-weight:bold;" id="candidateAddress"></label>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 clearMaginPadding">
                    <!-- note -->
                    <div class="row col-md-12 clearMaginPadding">
                        <div class="row col-md-12 clearMaginPadding">
                            <div class="col-md-2">
                                <h5>Note</h5>
                            </div>
                            <div class="col-md-10 ">
                                <textarea class="note" rows="3" style="width: 100%;"></textarea>
                            </div>

                        </div>
                        <div class="row col-md-12 clearMaginPadding">
                            <div class="col-md-2 col-sm-3 col-xs-3">
                                <h5>Action Type</h5>
                            </div>
                            <div class="col-md-7 col-xs-6 col-sm-6 ">
                                <select class="form-control status" id="status">
                                    <option>Check Isurance Number</option>
                                    <option>Call Reference</option>
                                    <option>Email Reference</option>
                                    <option>Call Candidate</option>
                                    <option>Email Candidate</option>
                                    <option>Other</option>
                                </select>
                            </div>
                            <div class="col-md-3  col-xs-3 col-sm-3 ">
                                <input type="submit" value="Log" class="btnFinish">
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>

        <!-- end general information -->
        <!-- Company -->

        <div class="col-md-12 clearMaginPadding" style="background-color: #666666;margin-top: 1em;">
            <ul class="nav nav-tabs" id="navCompanies"></ul>
        </div>
        <div class="tab-content" id="companyInformation">
            <div id="company_comId" class="tab-pane fade companyInfo">
                <form class="col-md-12 clearMaginPadding" style="padding-bottom: 1em" onsubmit="submitCompanyData('company_comId')">
                    <div class="col-md-8 clearMaginPadding">
                        <!-- information company -->
                        <div class="row col-md-12">
                            <div class="col-md-12 clearMaginPadding" style="padding-top: 1em">
                                <div style="padding-left: 1em">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <label>Website:</label>
                                            <label style="font-weight:bold;" id="companyWebsite">Swilife.com</label>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <label>Address:</label>
                                            <label style="font-weight:bold;" id="companyAddress">Số 12, Ngõ 641, Đường Phạm Văn Đồng</label>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <label>Name*:</label>
                                            <label style="font-weight:bold;" id="companyName">Swilife</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultName" id="nameTrue" value="True" class="minimal">
                                                    True
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultName" id="nameFalse" value="False" class="minimal">
                                                    False
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultName" id="nameNA" value="N/A" class="minimal" checked>
                                                    N/A
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <label>Job title*:</label>
                                            <label style="font-weight:bold;" id="companyJobtitle">Developer</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultJobTitle" id="jobTitleTrue" value="True" class="minimal">
                                                    True
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultJobTitle" id="jobTitleFalse" value="False" class="minimal">
                                                    False
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultJobTitle" id="jobTitleTrueNA" value="N/A" class="minimal" checked>
                                                    N/A
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <label>Startdate:</label>
                                            <label style="font-weight:bold;" id="companyStartDate">12-2-2018</label>
                                            <label>Stopdate*:</label>
                                            <label style="font-weight:bold;" id="companyStopDate">17-2-2018</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultPeriod" id="periodTrue" value="True" class="minimal">
                                                    True
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultPeriod" id="periodFalse" value="False" class="minimal">
                                                    False
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultPeriod" id="periodTrueNA" value="N/A" class="minimal" checked>
                                                    N/A
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <label>Job Duties*:</label>
                                            <label style="font-weight:bold;" id="companyJobDuties">Phat trien website</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultJobDuties" id="dutiesTrue" value="True" class="minimal">
                                                    True
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultJobDuties" id="dutiesFalse" value="False" class="minimal">
                                                    False
                                                </label>
                                                <label class="col-md-4">
                                                    <input type="radio" name="checkResultJobDuties" id="dutiesNA" value="N/A" class="minimal" checked>
                                                    N/A
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end information company -->
                        <!-- reference -->
                        <div style="overflow: scroll;height: 25em;width: 100%;">
                            <div class="col-md-12 ">
                                <!-- reference1 -->
                                <div class="row col-md-12 clearMaginPadding" id="referenceInformation">
                                    <div class="row col-md-12" id="referenceForm_0">
                                        <div class="row col-md-12">
                                            <h4>Reference</h4>
                                        </div>
                                        <div class="row col-md-12 clearMaginPadding">
                                            <div class="col-md-6 clearMaginPadding">
                                                <label>Name:</label>
                                                <label style="font-weight:bold;" id="referenceName">Nguyen Hoang Nam</label>
                                                <br>
                                                <label>Phone:</label>
                                                <label style="font-weight:bold;" id="referencePhone">0989989959</label>
                                                <br>
                                                <label>Email:</label>
                                                <label style="font-weight:bold;" id="referenceEmail">namnh@gmail.com</label>
                                            </div>
                                            <div class="col-md-6">
                                                <label>Job title:</label>
                                                <label style="font-weight:bold;" id="referenceJobTitle">Director</label>
                                                <br>
                                                <label>Relationship:</label>
                                                <label style="font-weight:bold;" id="referenceRelationship">supervisor</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row col-md-12" id="referenceForm_1">
                                        <div class="row col-md-12">
                                            <h4>Reference</h4>
                                        </div>
                                        <div class="row col-md-12 clearMaginPadding">
                                            <div class="col-md-6 clearMaginPadding">
                                                <label>Name:</label>
                                                <label style="font-weight:bold;" id="referenceName">Nguyen Hoang Nam</label>
                                                <br>
                                                <label>Phone:</label>
                                                <label style="font-weight:bold;" id="referencePhone">0989989959</label>
                                                <br>
                                                <label>Email:</label>
                                                <label style="font-weight:bold;" id="referenceEmail">namnh@gmail.com</label>
                                            </div>
                                            <div class="col-md-6">
                                                <label>Job title:</label>
                                                <label style="font-weight:bold;" id="referenceJobTitle">Director</label>
                                                <br>
                                                <label>Relationship:</label>
                                                <label style="font-weight:bold;" id="referenceRelationship">supervisor</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 clearMaginPadding" style="margin-top: 1em">
                        <!-- note -->
                        <div class="row col-md-12 clearMaginPadding">
                            <div class="col-md-2 clearMaginPadding">
                                <h5>Note</h5>
                            </div>
                            <div class="col-md-10 clearMaginPadding">
                                <textarea class="note" rows="10" style="width: 91.5%;"></textarea>
                            </div>
                        </div>
                        <!-- status -->
                        <div class="row col-md-12 clearMaginPadding">
                            <div class="col-md-2 clearMaginPadding">
                                <h5>
                                    Status:
                                </h5>
                            </div>
                            <div class="col-md-10 col-sm-10 col-xs-10 " style="padding-left: 0;padding-right: 3em">
                                <select class="form-control status select2" id="status">
                                    <option value="Processing">Processing</option>
                                    <option value="Completed">Completed</option>
                                    <option value="Closed">Closed</option>
                                    <option value="Pending">Pending</option>
                                </select>
                            </div>
                            <div class="col-md-12 col-sm-12 col-xs-12" style="padding-right: 3em">
                                <input type="submit" value="Submit" id="btnSubmitCompanyResult">
                            </div>


                        </div>
                    </div>
                </form>
            </div>
            <!-- end -->
        </div>
        <!--fooder-->
        <div class="row col-md-12 col-sm-12 col-xs-12 footer" style="margin:0px">
            <div class="col-md-6 col-sm-6 col-xs-12">
                <i class="fa fa-map-marker"></i>
                No 12, 641 Lane, Pham Van Dong Street, Ha Noi, Viet Nam
            </div>
            <div class="row col-md-6 col-sm-6 col-xs-12 footerright">
                <i>
                    <img src="~/Content/Image/e-mail.png" style="filter:white;height:1em"> contact@swilife.com
                </i>
                <i class="fa fa-phone"> 02439942285</i>
                <i><img src="~/Content/Image/logo PCS trang.png" class="logo"></i>
            </div>
        </div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/Scripts/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- SlimScroll -->
    <script src="~/Content/Js/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/Js/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/Js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/Js/demo.js"></script>
    <script src="~/Content/iCheck/icheck.min.js"></script>
    <script src="~/Scripts/utils.js"></script>
    <script src="~/Scripts/process-backgroundchecks-candidate.js"></script>

    <script>
        getAllCompanyInfo(@ViewBag.CandidateId);
        getCandidateId(@ViewBag.CandidateId);
    </script>

</body>
</html>
