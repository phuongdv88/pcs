@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title id="title">@ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!--for header-->
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Css/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Css/logo.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>

    <!-- Bootstrap 3.3.7 -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link href="~/Content/Ionicons/css/ionicons.min.css" rel="stylesheet" />

    <!-- DataTables -->
    <link href="~/Content/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/Content/Css/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/Css/skins/_all-skins.min.css" rel="stylesheet" />
    <link href="~/Content/Css/Specialist.css" rel="stylesheet" />
    <link href="~/Content/Css/ManageAccount.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>

<body class="hold-transition skin-blue sidebar-mini" style="padding-top:0px">
    <!--Header menu-->
    <div class="row col-md-12 col-sm-12 col-xs-12  header" style="margin:0;padding:0">
        <div class="col-md-3 col-sm-3 col-xs-3 " style=" background-color: #464646;padding: 0px;height:100%;">
            <img src="~/Content/Image/logo PCS trang.png" style="height:100%" />
        </div>
        <div class="col-md-9 col-sm-9 col-xs-9 " style=" background-color: #464646;padding: 0px;height:100%">

            <!-- Control Sidebar Toggle Button -->
            <div style="float: right;padding-top: 15px;padding-left: 15px;padding-right:1em" class="dropdown">
                <a href="#" data-toggle="control-sidebar">
                    <i class="fa fa-gears"></i>
                </a>
                <div class="dropdown-content sidenav">
                    <a href="#" data-toggle="modal" onclick="return getProfile();">
                        <b>
                            <i class="fa fa-user"></i> Change profile
                        </b>
                    </a>

                    <a href="#" data-toggle="modal" data-target="#changePassModal">

                        <b>
                            <i class="fa fa-edit"></i>Change Password
                        </b>
                    </a>

                    <a href=@Url.Action("Login", "Home")>
                        <b>
                            <i class="glyphicon glyphicon-log-out"></i>Log out
                        </b>
                    </a>
                </div>
            </div>
            <!-- form edit pass -->
            <div class="modal fade" id="changePassModal" role="dialog" tabindex="-1">
                <div class="modal-dialog" style="position:unset">
                    <!-- Modal content-->
                    <div class="modal-content" style="width: 100%;padding: 2em 2em 0 2em">
                        <div class="modal-header" style="padding-left: 0px;">
                            <h4>Change PassWord</h4>
                        </div>
                        <form onsubmit="updatePassword(); return false;">
                            <div class="modal-body form-edit">
                                <input id="UserName" name="UserName" hidden>
                                <label>Old Password</label><br />
                                <input type="password" id="oldPassword" placeholder="Password old.." required>
                                <label>New Password</label><br />
                                <input type="password" id="newPassword" placeholder="Password.." required>
                                <label>Confirm Password</label><br />
                                <input type="password" id="confirmPassword" placeholder="Confirm password.." required>
                                <div id="changePasswordMessage"></div>
                                <div class="modal-footer">
                                    <button type="submit" class="btn btn-primary" id="btnEditPass">Save</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- end form edit pass -->
            <!-- form uploadFile -->
            <div class="modal fade" id="uploadFileModal" role="dialog" tabindex="-1">
                <div class="modal-dialog" style="position:unset">
                    <!-- Modal content-->
                    <div class="modal-content" style="width: 100%;padding: 2em 2em 0 2em">
                        <div class="modal-header" style="padding-left: 0px;">
                            <h4>Upload Report</h4>
                        </div>
                        <div class="modal-body form-edit">
                            <input id="candidateId" name="candidateId" hidden>
                            <!--Form upload File-->
                            <div class="row">
                                <input class="col-md-12" id="fileInput" type="file" accept="application/pdf">
                            </div>
                            <div class="row">
                                <label>Change Status</label>
                                <div class="form-group col-md-12">
                                    <select class="form-control select2" id="candidateStatus" required>
                                        <option value="Completed">Completed</option>
                                        <option value="Closed">Closed</option>
                                    </select>
                                </div>
                            </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-primary" id="btnUpload" onclick="uploadFile(); return false;">Upload file</button>
                                </div>
                            </div>
                    </div>
                </div>
            </div>
            <!-- end form uploadFile -->
            <!-- form edit information -->
            <div class="modal fade" id="changeProfileModal" role="dialog" tabindex="-1">
                <div class="modal-dialog modal-lg" style="position:unset">
                    <!-- Modal content-->
                    <div class="modal-content" style="width: 100%;padding:  2em 2em 0 2em">
                        <div class="modal-header" style="padding-left: 0px;">
                            <h4 class="modal-title">Change Profile</h4>
                        </div>
                        <form onsubmit="editProfile(); return false;">

                            <div class="modal-body form-edit">
                                <label>First name</label><br />
                                <input type="text" id="specialistFirstName" placeholder="Firstname.." required>
                                <label>Middle name</label><br />
                                <input type="text" id="specialistMiddleName" placeholder="Firstname..">
                                <label>Last name</label><br />
                                <input type="text" id="specialistLastName" placeholder="Lastname.." required>
                                <label> Email</label><br />
                                <input type="email" id="specialistEmail" placeholder="Email.." required>
                                <label>Phone number</label><br />
                                <input type="text" id="specialistPhoneNumber" pattern="^[0-9\-\+]{9,15}$" placeholder="Phone number.." required>
                                <div class="modal-footer">
                                    <button type="submit" class="btn btn-primary" id="btnEditProfile">Save</button>
                                    @*<input />*@
                                </div>
                            </div>

                        </form>
                    </div>
                </div>
            </div>

            <!-- end form edit information -->

            <div style="float: right;padding-top: 13px;height: 100%;width:auto">
                <!-- The user image in the navbar-->
                <i class="fa fa-user" style="color:silver" id="welcomeUser"> Welcome @ViewBag.Title</i>
                <span></span>
            </div>
        </div>
    </div>
    <!-- body-->
    <div class=" col-md-12 col-sm-12 col-xs-12 clearPanddingMargin" style="display:flex;min-height:890px">
        <div class=" col-md-2 col-sm-2 col-xs-12  slide-bar" style="padding:0px">
            <div class="col-md-12  col-sm-12 col-xs-12 list">

                <a href="#candidate-management" data-toggle="tab">
                    <div class="col-md-12 col-sm-12 candidate-management item">
                        <img src="~/Content/Image/iconmanager.png" />
                        Candidate Management
                    </div>
                </a>


                <a href="#task-management" data-toggle="tab" id="bs-task-management">
                    <div class="col-md-12 col-sm-12 task-management item">
                        <img src="~/Content/Image/iconreport.png" />
                        My Task
                    </div>
                </a>

            </div>
        </div>
        <div class=" col-md-10 col-sm-10 col-xs-12  " style="padding-top:1em">

            <div class="tab-content">
                <div id="candidate-management" class="tab-pane fade in active">
                    <div class="wrapper">
                        <!-- Content Wrapper. Contains page content -->
                        <div class="content-wrapper">
                            <!-- Main content -->
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="box">
                                        <div class="box-header">
                                            <h3 class="box-title">Candidate Information</h3>
                                            <div>
                                                <button type="button" class="btn btn-primary btn-lg" id="btnRefresh" title="Refresh" onclick="return getAvailableCandidate();"><span class="glyphicon glyphicon-refresh"></span></button>
                                            </div>
                                        </div>
                                        <!-- /.box-header -->
                                        <div class="box-body">
                                            <table id="candidates" class="table table-bordered table-striped">
                                                <thead>
                                                    <tr>
                                                        <th>Index</th>
                                                        <th>Name</th>
                                                        <th>Email</th>
                                                        <th>Phone Number</th>
                                                        <th>Status</th>
                                                        <th>Created Date</th>
                                                        <th>Name Company</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="tbody"></tbody>
                                                <tfoot>
                                                    <tr>
                                                        <th>Index</th>
                                                        <th>Name</th>
                                                        <th>Email</th>
                                                        <th>Phone Number</th>
                                                        <th>Status</th>
                                                        <th>Created Date</th>
                                                        <th>Completed Date</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </tfoot>
                                            </table>
                                        </div>
                                        <!-- /.box-body -->
                                    </div>
                                    <!-- /.box -->
                                </div>
                                <!-- /.col -->
                            </div>
                            <!-- /.row -->
                            <!-- /.content -->
                        </div>
                    </div>
                </div>
                <div id="task-management" class="tab-pane fade in">
                    <!--chart-->
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class=" box box-success barchart">
                            <div class="box-header with-border">
                                <h3 id="textchart" class="box-title">Bar Chart</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>

                                </div>
                            </div>
                            <div class="box-body">
                                <div class="chart">
                                    <canvas id="barChart" style="height:230px"></canvas>

                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <div class="col-md-6 col-sm-12 col-xs-12">
                                            <div class="col-md-3 col-sm-3 col-xs-3" style="height: 2em;width: 2em;background-color: rgb(255, 51, 102) ">

                                            </div>
                                            <div class="col-md-9 col-sm-9 col-xs-9"> Registered </div>
                                        </div>
                                        <div class="col-md-6 col-sm-12 col-xs-12">
                                            <div class="col-md-3 col-sm-3 col-xs-3" style="height: 2em;width: 2em;background-color:#3366ff">

                                            </div>
                                            <div class="col-md-9 col-sm-9 col-xs-9"> Complete </div>

                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!-- /.box-body -->
                        </div>
                    </div>
                    <!--end chart-->

                    <div class="wrapper" style="width:100%">
                        <!-- Content Wrapper. Contains page content -->
                        <div class="content-wrapper">
                            <!-- Main content -->
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="box">
                                        <div class="box-header">
                                            <h3 class="box-title">My Task</h3>
                                            <div>
                                                <button type="button" class="btn btn-primary btn-lg" id="btnRefreshmyTask" title="Refresh" onclick="return getMyCandidate();"><span class="glyphicon glyphicon-refresh"></span></button>
                                            </div>
                                        </div>
                                        
                                        <!-- /.box-header -->
                                        <div class="box-body">
                                            <table id="myTask" class="table table-bordered table-striped">
                                                <thead>
                                                    <tr>
                                                        <th>Index</th>
                                                        <th>Company</th>
                                                        <th>Full Name</th>
                                                        <th>Email</th>
                                                        <th>Phone Number</th>
                                                        <th>Status</th>
                                                        <th>Created Date</th>
                                                        <th>Completed Date</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="tbody"></tbody>
                                                <tfoot>
                                                    <tr>
                                                        <th>Index</th>
                                                        <th>Company</th>
                                                        <th>Full Name</th>
                                                        <th>Email</th>
                                                        <th>Phone Number</th>
                                                        <th>Status</th>
                                                        <th>Created Date</th>
                                                        <th>Completed Date</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </tfoot>
                                            </table>
                                        </div>
                                        <!-- /.box-body -->
                                    </div>
                                    <!-- /.box -->
                                </div>
                                <!-- /.col -->
                            </div>
                            <!-- /.row -->
                            <!-- /.content -->
                        </div>
                    </div>
                </div>
            </div>
          
            <!-- Candidate's information Modal -->
            <div class="modal fade" id="candiateEmailInfoModal" tabindex="-1" role="dialog" aria-hidden="true">
                <div class="modal-dialog modal-lg" style="position:unset">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                            <h4 class="modal-title" id="infoCandidateModalLabel">Please email this content to Candidate</h4>
                        </div>
                        <div class="modal-body">
                            <div><p style="margin-top:0pt; margin-bottom:8pt; text-align:justify; line-height:108%; font-size:12pt"><span style="font-family:Arial">Hi&nbsp;<span id="candidateName"></span></span><span style="font-family:Arial">,</span></p><p style="margin-top:0pt; margin-bottom:8pt; text-align:justify; line-height:108%; font-size:12pt"><span style="font-family:Arial">You have received this message because you </span><span style="font-family:Arial">are </span><span style="font-family:Arial">registered on</span><span style="font-family:Arial">&#xa0;</span><span style="font-family:Arial">Professional Checking System. Professional Checking System owned by SWILIFE is specialized in Employment Verification service. Our friendly, knowledgeable customer support team is ready to help ensure your Employment Verification go smoothly.</span></p><p style="margin-top:0pt; margin-bottom:8pt; text-align:justify; line-height:108%; font-size:13pt"><span style="font-family:Arial; font-size:12pt">Please login at: </span><a href="https://professionalcheckingsystem.com" style="text-decoration:none"><span style="font-family:Arial; font-size:12pt; text-decoration:underline; color:#0563c1">https://professionalcheckingsystem.com</span></a><span style="font-family:Arial; font-size:12pt"> and fill up all information </span><span style="font-family:Arial; font-size:12pt">fields</span><span style="font-family:Arial; font-size:12pt"> </span><span style="font-family:Arial; font-size:12pt">before <span id="lockoutDateUtc"></span></span><span style="font-family:Arial; font-size:12pt">&nbsp;to ensure smooth verification process.</span></p><p style="margin-top:0pt; margin-bottom:8pt; text-align:justify; line-height:108%; font-size:12pt"><span style="font-family:Arial">Use the following values when prompted to log in: </span></p><p style="margin-top:0pt; margin-bottom:8pt; text-indent:22.5pt; text-align:justify; line-height:108%; font-size:12pt"><span style="font-family:Arial">User Name:&nbsp;<span id="userNameInfo"></span></span></p><p style="margin-top:0pt; margin-bottom:8pt; text-indent:22.5pt; text-align:justify; line-height:108%; font-size:12pt"><span style="font-family:Arial">Password:&nbsp;<span id="passwordRaw"></span></span></p><p style="margin-top:0pt; margin-bottom:8pt; text-align:justify; line-height:108%; font-size:12pt"><span style="font-family:Arial; font-style:italic; text-decoration:underline">Note:</span><span style="font-family:Arial"> </span><span style="font-family:Arial">We only conduct Verification Process according to your company’s order which is allowed by candidates. If you don’t know about this proce</span><span style="font-family:Arial">ss, please contact your company. W</span><span style="font-family:Arial">e shall not </span><span style="font-family:Arial">hold</span><span style="font-family:Arial"> liable for any </span><span style="font-family:Arial">candidates’ claim</span><span style="font-family:Arial"> of </span><span style="font-family:Arial">collecting personal information without </span><span style="font-family:Arial">owner’s </span><span style="font-family:Arial">permission</span><span style="font-family:Arial">.</span><span style="font-family:Arial"> Finally, we ensur</span><span style="font-family:Arial">e hold all the information you supplied in complete confidence, and without </span><span style="font-family:Arial">your company</span><span style="font-family:Arial">’s consent in writing shall not be revealed any part to any other third parties, at any time, nor used for any purpose other than the performance </span><span style="font-family:Arial">in the</span><span style="font-family:Arial"> Agreement</span><span style="font-family:Arial"> we signed with your company</span><span style="font-family:Arial">.</span></p><p style="margin-top:0pt; margin-bottom:8pt; text-align:justify; line-height:108%; font-size:12pt"><span style="font-family:Arial">&#xa0;</span></p><p style="margin-top:0pt; margin-bottom:8pt; line-height:108%; font-size:12pt"><span style="font-family:Arial">SWILIFE JOINT STOCK COMPANY</span></p><p style="margin-top:0pt; margin-bottom:8pt; line-height:108%; font-size:12pt"><span style="font-family:Arial">No.12, 641 </span><span style="font-family:Arial">Lane, Pham Van Dong Street, Co Nhue 1 Ward, Bac Tu Liem District, Hanoi</span></p></div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-primary" data-dismiss="modal">OK</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--fooder-->
    <div class="row col-md-12 col-sm-12 col-xs-12 footer" style="margin:0px">
        <div class="col-md-6 col-sm-6 col-xs-12">
            <i class="fa fa-map-marker"></i>
            No 12, 641 Lane, Pham Van Dong Street, Ha Noi, Viet Nam
        </div>
        <div class="row col-md-6 col-sm-6 col-xs-12 footerright">
            <i>
                <img src="~/Content/Image/e-mail.png" style="filter:white;height:1em"> contact@swilife.com
            </i>
            <i class="fa fa-phone"> 02439942285</i>
            <i><img src="~/Content/Image/logo PCS trang.png" class="logo"></i>
        </div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/Scripts/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- DataTables -->
    <script src="~/Content/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <!-- SlimScroll -->
    <script src="~/Content/Js/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/Js/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/Js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/Js/demo.js"></script>
    <script src="~/Scripts/manage-specialist-account-1.0.0.js"></script>
    <!--chart-->
    <script src="~/Scripts/Chart.js"></script>

    <!-- page script -->
    <script>
        $(function () {
            _setCurrentRecruitId(@ViewBag.SpecialistId);
            getAvailableCandidate();
            getMyCandidate();
            setUserLoginName("@Session["UserName"].ToString()");
        })
    </script>

</body>
</html>
